{
  "name": "@opentelemetry/instrumentation-azure-functions",
  "version": "0.1.0",
  "description": "OpenTelemetry Azure Functions automatic instrumentation package.",
  "keywords": [
    "azure",
    "azure-functions",
    "serverless",
    "instrumentation",
    "nodejs",
    "opentelemetry"
  ],
  "homepage": "https://github.com/open-telemetry/opentelemetry-js-contrib/tree/main/plugins/node/opentelemetry-instrumentation-azure-functions#readme",
  "license": "Apache-2.0",
  "author": "OpenTelemetry Authors",
  "repository": "open-telemetry/opentelemetry-js-contrib",
  "bugs": {
    "url": "https://github.com/open-telemetry/opentelemetry-js-contrib/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "tsc -p .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "precompile": "tsc --version && lerna run version:update --scope @opentelemetry/instrumentation-azure-functions --include-dependencies",
    "prepublishOnly": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json --require '@opentelemetry/contrib-test-utils' 'test/**/*.test.ts'",
    "version:update": "node ../../../scripts/version-update.js",
    "prewatch": "npm run precompile",
    "watch": "tsc -w"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "devDependencies": {
    "@azure/functions": "^4.0.0",
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/contrib-test-utils": "^0.39.0",
    "@types/mocha": "7.0.2",
    "@types/node": "18.15.3",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "rimraf": "5.0.5",
    "ts-mocha": "10.0.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "@opentelemetry/instrumentation": "^0.51.0"
  }
}
